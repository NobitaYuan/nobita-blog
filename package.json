{
    "type": "module",
    "private": true,
    "packageManager": "pnpm@9.10.0",
    "scripts": {
        "build": "cross-env NODE_ENV=production vite-ssg build && copy _dist_redirects docs/_redirects",
        "build-old": "npm run static && cross-env NODE_ENV=production vite-ssg build && esno ./scripts/rss.ts && cp _dist_redirects dist/_redirects",
        "dev": "vite --port 3333",
        "dev-op": "vite --port 3333 --open",
        "preview": "vite preview",
        "static": "rimraf temp && degit antfu/static temp --force && esno scripts/copy-sponsors",
        "redirects": "esno scripts/redirects.ts",
        "lint": "eslint .",
        "compress": "esno scripts/img-compress-staged.ts",
        "prepare": "simple-git-hooks",
        "cz": "cz"
    },
    "dependencies": {
        "@vueuse/core": "^11.1.0",
        "axios": "^1.7.7",
        "dayjs": "^1.11.13",
        "floating-vue": "^5.2.2",
        "matter-attractors": "^0.1.6",
        "matter-js": "^0.20.0",
        "nprogress": "^0.2.0",
        "shiki-magic-move": "^0.4.4",
        "vite-plugin-vue-devtools": "^7.4.5",
        "vue": "^3.5.6",
        "vue-router": "4.2.5",
        "vue-router-better-scroller": "^0.0.0"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^3.6.2",
        "@iconify/json": "^2.2.249",
        "@octokit/rest": "^21.0.2",
        "@shikijs/markdown-it": "^1.17.7",
        "@shikijs/twoslash": "^1.17.7",
        "@types/d3-hierarchy": "^3.1.7",
        "@types/degit": "^2.8.6",
        "@types/diacritics": "^1.3.3",
        "@types/fs-extra": "^11.0.4",
        "@types/markdown-it": "^14.1.2",
        "@types/markdown-it-link-attributes": "^3.0.5",
        "@types/nprogress": "^0.2.3",
        "@types/prompts": "^2.4.9",
        "@unhead/vue": "^1.11.6",
        "@unocss/reset": "^0.62.3",
        "@vitejs/plugin-vue": "^5.1.3",
        "commitizen": "^4.3.0",
        "cross-env": "^7.0.3",
        "cz-git": "^1.9.4",
        "d3-hierarchy": "^3.1.2",
        "degit": "^2.8.4",
        "diacritics": "^1.3.0",
        "eslint": "^9.10.0",
        "eslint-plugin-format": "^0.1.2",
        "esno": "^4.7.0",
        "fast-glob": "^3.3.2",
        "feed": "^4.2.2",
        "fs-extra": "^11.2.0",
        "gray-matter": "^4.0.3",
        "lint-staged": "^15.2.10",
        "markdown-it": "^14.1.0",
        "markdown-it-anchor": "^9.2.0",
        "markdown-it-github-alerts": "^0.3.0",
        "markdown-it-link-attributes": "^4.0.1",
        "markdown-it-magic-link": "^0.1.4",
        "markdown-it-table-of-contents": "^0.6.0",
        "p5i": "^0.4.2",
        "picocolors": "^1.1.0",
        "pinia": "^2.2.2",
        "pnpm": "^9.10.0",
        "prompts": "^2.4.2",
        "rimraf": "^6.0.1",
        "sharp": "0.32.6",
        "shiki": "^1.17.7",
        "simple-git": "^3.26.0",
        "simple-git-hooks": "^2.11.1",
        "typescript": "^5.6.2",
