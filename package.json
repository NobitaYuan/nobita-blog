        "vite-ssg": "^0.23.8",
        "vite-svg-loader": "^5.1.0"
    },
    "simple-git-hooks": {
        "pre-commit": "npx lint-staged"
    },
    "lint-staged": {
        "*": ""
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-git"
        }
    }
}
